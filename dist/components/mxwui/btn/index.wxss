.mxbtn{display:flex;align-items:center;justify-content:center;height:88rpx;border-radius:4rpx;box-sizing:border-box;font-size:36rpx;font-family:PingFangSC-Semibold,PingFang SC;padding:0;width:702rpx}.mxbtn.mxbtn-primary{color:#fff;background:#ca0e2d}.mxbtn.mxbtn-primary.mxbtn-hover{background:rgba(202,14,45,.8)}.mxbtn.mxbtn-primary.mxbtn-unClick{color:rgba(255,255,255,.5);background:rgba(202,14,45,.5)}.mxbtn.mxbtn-primary.mxbtn-disabled{background:#9aa0b1}.mxbtn.mxbtn-ghost{color:#ca0e2d;border:2rpx solid #ca0e2d;background:0 0}.mxbtn.mxbtn-ghost.mxbtn-hover{color:rgba(202,14,45,.8);border-color:rgba(202,14,45,.7)}.mxbtn.mxbtn-ghost.mxbtn-unClick{color:rgba(202,14,45,.4);border-color:rgba(202,14,45,.4)}.mxbtn.mxbtn-ghost.mxbtn-disabled{color:#9aa0b1;border-color:#9aa0b1}.mxbtn.mxbtn-default{color:#ca0e2d;border:2rpx solid #ca0e2d;background:rgba(202,14,45,.05)}.mxbtn.mxbtn-default.mxbtn-hover{color:rgba(202,14,45,.8);background:rgba(202,14,45,.03)}.mxbtn.mxbtn-default.mxbtn-unClick{color:rgba(202,14,45,.4);border-color:rgba(202,14,45,.4)}.mxbtn.mxbtn-default.mxbtn-disabled{color:#9aa0b1;border-color:#9aa0b1;background:rgba(154,160,177,.05)}.mxbtn.mxbtn-round{border-radius:44rpx}.mxbtn.mxbtn-small{height:56rpx;font-size:28rpx}