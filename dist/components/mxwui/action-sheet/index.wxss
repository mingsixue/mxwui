.mx-dialog-head{display:flex;align-items:center;justify-content:center;width:100vw;height:106rpx;font-size:36rpx;font-family:PingFangSC-Semibold,PingFang SC;font-weight:600;color:#040a23;background:#f8f9fa}.mx-dialog-cons .list{display:block;width:702rpx;margin:0 auto}.mx-dialog-cons .list .item{position:relative;display:flex;align-items:center;justify-content:center;width:702rpx;height:104rpx;font-size:36rpx;color:#040a23;box-shadow:inset 0 -1px 0 0 #e1e5ec}.mx-dialog-cons .list .item:last-child{box-shadow:none}.mx-dialog-cons .list .item .txt{display:flex;align-items:center;justify-content:center;height:104rpx}.mx-dialog-cons .list .item .icons{display:flex;align-items:center;justify-content:center;margin-right:8rpx}.mx-dialog-cons .list .item .icons .icon{width:48rpx;height:48rpx}.mx-dialog-cons .line{width:750rpx;height:16rpx;background:#f8f9fa}.mx-dialog-cons .cancel{height:96rpx;display:flex;align-items:center;justify-content:center;font-size:36rpx;color:#9aa0b1}