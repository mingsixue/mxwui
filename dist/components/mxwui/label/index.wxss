.mx-label{display:inline-flex;align-items:center;justify-content:center;min-width:112rpx;height:72rpx;border-radius:4rpx;padding:0 24rpx;background:rgba(154,160,177,.1)}.mx-label .text{color:#040a23;font-size:28rpx}.mx-label.mx-label-single{background:rgba(202,14,45,.1)}.mx-label.mx-label-single .text{color:#ca0e2d}.mx-label.mx-label-multiple{position:relative;background:rgba(202,14,45,.1);overflow:hidden}.mx-label.mx-label-multiple .text{color:#ca0e2d}.mx-label.mx-label-multiple .select{position:absolute;top:0;right:0;z-index:1;width:0;height:0;font-size:0}.mx-label.mx-label-multiple::after{content:'';position:absolute;top:0;right:0;z-index:2;width:20rpx;height:20rpx;background:url(http://cdn.mingsixue.com/xcx/MXWUI/multi.png) no-repeat;background-size:cover}.mx-label.mx-label-disabled .text{color:#9aa0b1!important}